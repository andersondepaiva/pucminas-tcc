{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "gateway"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "https://sca-discoveryserver.herokuapp.com/eureka/",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false,
      "registryFetchIntervalSeconds": 10,
      "instanceInfoReplicationIntervalSeconds": 10
    },
    "instance": {
      "instanceId": "localhost:8000",
      "hostName": "localhost"
    }
  }
}
