{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "spring": {
    "application": {
      "name": "gateway"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false,
      "registryFetchIntervalSeconds": 10,
      "instanceInfoReplicationIntervalSeconds": 10
    },
    "instance": {
      "port": 8000,
      "instanceId": "localhost:8000",
      "hostName": "localhost"
    }
  }
}
